{
  "name": "yubista2020",
  "version": "1.0.0",
  "description": "This is our company's brand new project called YubiSta and it has been developed in Mongolian branch.",
  "scripts": {
    "ng": "ng",
    "ng-test": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "ng-build": "ng build",
    "ng-build-watch": "npm run init-client-dev-env && ng build --watch",
    "e2e": "ng e2e",
    "ng-build-prod": "npm run init-client-prod-env && node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --prod",
    "build": "npm run build-ts && npm run lint && npm run ng-build",
    "build-prod": "NODE_OPTIONS=--max_old_space_size=4096 npm run build-ts && npm run lint && npm run ng-build-prod && npm run pre-compress",
    "start": "node -r dotenv/config dist/server/server.js dotenv_config_path=./src/server/config/.env.dev",
    "start:test": "node -r dotenv/config dist/server/server.js dotenv_config_path=./src/server/config/.env.test",
    "start:stage": "node -r dotenv/config dist/server/server.js dotenv_config_path=./src/server/config/.env",
    "start:prod": "node -r dotenv/config dist/server/server.js dotenv_config_path=./src/server/config/.env",
    "watch-node": "nodemon -r dotenv/config dist/server/server.js dotenv_config_path=./src/server/config/.env.dev",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Angular,Node\" -c \"cyan.bold,yellow.bold,green.bold\" \"npm run watch-ts\" \"npm run ng-build-watch\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc --p ./tsconfig.json && npm run copy-static-assets",
    "watch-ts": "tsc -w --p ./tsconfig.json",
    "lint": "./node_modules/.bin/eslint \"*/**/*.{js,ts}\" --quiet --fix  && echo 'Coding convention passed!'",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "copy-static-assets": "ts-node copy-static-assets.ts",
    "init-client-dev-env": "NODE_ENV=development ts-node init-client-env.ts",
    "init-client-prod-env": "NODE_ENV=production ts-node init-client-env.ts",
    "pre-compress": "ts-node compressor.ts",
    "postinstall": "ngcc"
  },
  "repository": {
    "type": "git",
    "url": "eysdevpro2@eysdevpro2.git.backlog.jp:/FIRST/yubista2020.git"
  },
  "author": "Undarmaa Baatarchuluun, Purevdondov Enkhbayar, Bat-Orshikh Baavgaikhuu",
  "license": "ISC",
