    "karma": "^5.0.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "karma-viewport": "^1.0.6",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.3",
    "protractor": "^5.4.3",
    "puppeteer": "^2.1.1",
    "shelljs": "^0.8.3",
    "ts-node": "^7.0.0",
    "typescript": "^3.7.5",
