{
  "name": "yubista2020",
  "version": "1.0.0",
  "description": "This is our company's brand new project called YubiSta and it has been developed in Mongolian branch.",
  "scripts": {
    "ng": "ng",
    "ng-test": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "ng-build": "ng build",
    "ng-build-watch": "ng build --watch",
    "ng-lint": "ng lint",
    "e2e": "ng e2e",
    "build": "npm run build-ts && npm run lint && npm run ng-build && npm run ng-lint",
    "start": "node -r dotenv/config dist/server/server.js dotenv_config_path=./src/server/config/.env.dev",
    "start:test": "node -r dotenv/config dist/server/server.js dotenv_config_path=./src/server/config/.env.test",
    "start:stage": "node -r dotenv/config dist/server/server.js dotenv_config_path=./src/server/config/.env",
    "start:prod": "node -r dotenv/config dist/server/server.js dotenv_config_path=./src/server/config/.env",
    "watch-node": "nodemon -r dotenv/config dist/server/server.js dotenv_config_path=./src/server/config/.env.dev",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Angular,Node\" -c \"cyan.bold,yellow.bold,green.bold\" \"npm run watch-ts\" \"npm run ng-build-watch\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc --p ./tsconfig.json",
    "watch-ts": "tsc -w --p ./tsconfig.json",
    "lint": "tsc --noEmit && ./node_modules/.bin/eslint \"**/*.{js,ts}\" --quiet --fix  && echo 'Coding convention passed!'",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "repository": {
    "type": "git",
    "url": "eysdevpro2@eysdevpro2.git.backlog.jp:/FIRST/yubista2020.git"
  },
  "author": "Undarmaa Baatarchuluun, Purevdondov Enkhbayar, Tenuun Khangaitan, Bat-Orshikh Baavgaikhuu",
  "license": "ISC",
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.21",
    "@angular/cli": "^8.3.21",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/body-parser": "^1.17.1",
    "@types/concurrently": "^4.1.0",
    "@types/cors": "^2.8.6",
    "@types/eslint": "^6.1.3",
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.45",
    "@types/jasmine": "^3.3.8",
    "@types/jasminewd2": "^2.0.3",
    "@types/morgan": "^1.7.37",
    "@types/node": "^8.9.4",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "codelyzer": "^5.0.0",
    "concurrently": "^5.0.2",
    "eslint": "^6.7.2",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-viewport": "^1.0.4",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.2",
    "protractor": "^5.4.0",
    "puppeteer": "^2.0.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.15.0",
    "typescript": "^3.5.3",
    "winston": "^3.2.1"
  },
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "^0.9.1"
  }
}
